@model List<UniversityMvc.Models.Customer>

<h2>Customers</h2>

<table border="1">
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>

    @foreach (var c in Model)
    {
        <tr>
            <td>@c.IdNumber</td>
            <td>@c.FirstName</td>
            <td>@c.LastName</td>
        </tr>
    }
</table>